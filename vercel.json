{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/sitemap\\.xml",
      "dest": "/api/index.ts"
    },
    {
      "src": "/sitemap-index\\.xml", 
      "dest": "/api/index.ts"
    },
    {
      "src": "/rss\\.xml",
      "dest": "/api/index.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}